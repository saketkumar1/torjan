<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="110dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/parentColleges"
    app:cardElevation="15dp"
    app:cardCornerRadius="55dp"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="15dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="5dp"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relLayoutColleges"
        >

        <TextView
            android:id="@+id/txtMedicalCollegeName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:fontFamily="@font/droid_sans"
            android:text="Gayathri Vidya Parishad Inst. of Health Care Med. Tech. Visakhapatnam Inst. of Health Care  Med. Tech. "
            android:textSize="18sp"
            android:textColor="#fff"/>

    </RelativeLayout>




